{
  "name": "slackbot",
  "version": "0.1.0",
  "description": "The CodeNetwork SlackBot",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./src/app.js",
    "test": "npm run unit-tests && npm run lint",
    "postinstall": "npm run compile",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "unit-tests": "./node_modules/.bin/babel-node ./node_modules/.bin/mocha \"src/tests/**/*Test.js\" --reporter nyan",
    "compile": "./node_modules/.bin/babel src --out-dir target --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codenetwork/SlackBot.git"
  },
  "keywords": [
    "slack"
  ],
  "author": "Code Network",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codenetwork/SlackBot/issues"
  },
  "homepage": "https://github.com/codenetwork/SlackBot#readme",
  "dependencies": {
    "@slack/client": "^3.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "mocha": "^3.0.2"
  }
}
